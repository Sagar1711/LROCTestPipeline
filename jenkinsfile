pipeline {
    agent {dockerfile true}
    stages {
        stage('Checkout') {
            checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Sagar1711/LROCTestPipeline.git']]])
        }
        stage('Build Env') {
            steps {
                sh 'uname --all'
                sh 'gcc --version'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World'
            }
        }
    }
}

