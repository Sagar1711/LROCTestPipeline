pipeline {
    parameters {
        string(name: "rc_zip_url", defaultValue: true, description: "Sample boolean parameter")
        string(name: "rc_tar_url", defaultValue: "ssbostan", trim: true, description: "Sample string parameter")
    }
    agent {dockerfile true}
    stages {
        stage('Build Env') {
            steps {
                sh 'uname --all'
                sh 'gcc --version'
            }
        }
    }
}

